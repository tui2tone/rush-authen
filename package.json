{
	"name": "rush-auth-api",
	"version": "0.0.1",
	"description": "",
	"author": "",
	"private": true,
	"license": "UNLICENSED",
	"scripts": {
		"prebuild": "rimraf dist",
		"build": "nest build",
		"format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
		"prestart": "npm run setup",
		"start": "concurrently --handle-input -p \"[{name}]\" -n \"API,VIEW\" \"wait-on dist/main.js && nodemon\" \"tsc -w -p tsconfig.build.json\" ",
		"start:fullstack": "concurrently --handle-input -p \"[{name}]\" -n \"API,VIEW,ADMIN\" \"wait-on dist/main.js && nodemon\" \"tsc -w -p tsconfig.build.json\" \"cd admin && npm run build:watch\" ",
		"setup": "node scripts/auto-alias.js",
		"start:dev": "nest start --watch",
		"start:debug": "nest start --debug --watch",
		"start:prod": "node dist/main",
		"lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:cov": "jest --coverage",
		"test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
		"test:e2e": "jest --config ./test/jest-e2e.json",
		"schema:sync": "rm -rf dist && npm run build && ./node_modules/.bin/ts-node ./node_modules/.bin/typeorm schema:sync",
		"seed": "rm -rf dist && npm run build && ts-node ./node_modules/typeorm-seeding/dist/cli.js seed"
	},
	"dependencies": {
		"@nestjs/common": "^7.0.0",
		"@nestjs/core": "^7.0.0",
		"@nestjs/passport": "^7.1.0",
		"@nestjs/platform-express": "^7.4.2",
		"@nestjs/platform-socket.io": "^7.4.2",
		"@nestjs/serve-static": "^2.1.3",
		"@nestjs/swagger": "^4.6.0",
		"@nestjs/typeorm": "^7.1.1",
		"@nestjs/websockets": "^7.4.2",
		"@nestjsx/crud": "^4.6.2",
		"@nestjsx/crud-typeorm": "^4.6.2",
		"axios": "^0.20.0",
		"cache-manager": "^3.3.0",
		"cache-manager-redis-store": "^2.0.0",
		"class-transformer": "^0.3.1",
		"class-validator": "^0.12.2",
		"client-oauth2": "^4.3.3",
		"dotenv": "^8.2.0",
		"express-http-proxy": "^1.6.2",
		"googleapis": "^59.0.0",
		"hbs": "^4.1.1",
		"module-alias": "^2.2.2",
		"nodemailer": "^6.4.11",
		"oidc-provider": "^6.29.5",
		"openid-client": "^3.15.10",
		"passport": "^0.4.1",
		"passport-http-bearer": "^1.0.1",
		"pg": "^8.3.2",
		"query-string": "^6.13.2",
		"randomstring": "^1.1.5",
		"reflect-metadata": "^0.1.13",
		"rimraf": "^3.0.2",
		"rxjs": "^6.5.4",
		"sequelize": "^6.3.5",
		"swagger-ui-express": "^4.1.4",
		"typeorm": "^0.2.25",
		"underscore": "^1.10.2"
	},
	"devDependencies": {
		"@nestjs/cli": "^7.0.0",
		"@nestjs/schematics": "^7.0.0",
		"@nestjs/testing": "^7.4.2",
		"@types/express": "^4.17.3",
		"@types/jest": "25.1.4",
		"@types/node": "^13.9.1",
		"@types/supertest": "^2.0.8",
		"@typescript-eslint/eslint-plugin": "^2.23.0",
		"@typescript-eslint/parser": "^2.23.0",
		"eslint": "^6.8.0",
		"eslint-config-prettier": "^6.10.0",
		"eslint-plugin-import": "^2.20.1",
		"glob": "^7.1.6",
		"jest": "^25.1.0",
		"prettier": "^1.19.1",
		"supertest": "^4.0.2",
		"ts-jest": "25.2.1",
		"ts-loader": "^6.2.1",
		"ts-node": "^8.6.2",
		"tsconfig-paths": "^3.9.0",
		"typeorm-seeding": "^1.6.1",
		"typescript": "^3.7.4",
		"wait-on": "^5.2.0",
		"write-json-file": "^4.3.0"
	},
	"jest": {
		"moduleFileExtensions": [
			"js",
			"json",
			"ts"
		],
		"rootDir": "src",
		"testRegex": ".spec.ts$",
		"transform": {
			"^.+\\.(t|j)s$": "ts-jest"
		},
		"coverageDirectory": "../coverage",
		"testEnvironment": "node"
	},
	"_moduleAliases": {
		"@adapters": "dist/adapters",
		"@additional-fields": "dist/additional-fields",
		"@auth": "dist/auth",
		"@clients": "dist/clients",
		"@config": "dist/config",
		"@core": "dist/core",
		"@decorators": "dist/decorators",
		"@guard": "dist/guard",
		"@healthcheck": "dist/healthcheck",
		"@interaction": "dist/interaction",
		"@interceptors": "dist/interceptors",
		"@projects": "dist/projects",
		"@register": "dist/register",
		"@roles": "dist/roles",
		"@seeds": "dist/seeds",
		"@setting": "dist/setting",
		"@setup": "dist/setup",
		"@users": "dist/users",
		"@utils": "dist/utils"
	}
}
